<%- include('partials/_header') %> %>

    <a href="/">
      <button>INICIO</button>
    </a>

    <div class="d-flex">
      <% infoGame.forEach((infoGame, index) => { %>
        <h2><%= infoGame.equipoA %></h2>
      <% }) %>  
      
      <% playersA.forEach((playersA, index) => { %>
        <option><%=playersA.nro %></option>
      <% }) %>  
    </div>

    <div class="d-flex">
      
      <div class="col-2">
        <button id="cero" class="p-4 btn btn-danger">0</button>
        <button id="dos" class="p-4 btn btn-danger">2</button>
        <button id="uno" class="p-4 btn btn-danger">1</button>
        <button id="tres" class="p-4 btn btn-danger">3</button>
      </div>

      <h2 id="scoring" class="display-1 col-3"></h2> 
    </div>
    
    




  <script src="score.js"></script> 

</body>

</html>   